&pinctrl {

  i2c1_default: i2c1_default {
    group1 {
      psels = <NRF_PSEL(TWIM_SCL, 0, 12)>, 
              <NRF_PSEL(TWIM_SDA, 0, 14)>;
    };
  };

  i2c1_sleep: i2c1_sleep {
    group1 {
      psels = <NRF_PSEL(TWIM_SCL, 0, 12)>, 
              <NRF_PSEL(TWIM_SDA, 0, 14)>;
    };
    low-power-enable;
  };

};